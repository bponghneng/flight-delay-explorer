# Flight Delay Explorer Environment Configuration
# Copy this file to .env and fill in your actual values

# AviationStack API access key (required)
# Get your API key from: https://aviationstack.com/
AVIATIONSTACK_ACCESS_KEY=your_api_key_here

# API base URL (default: https://api.aviationstack.com/v1)
BASE_URL=https://api.aviationstack.com/v1

# Maximum retry attempts for failed requests (default: 3)
MAX_RETRIES=3

# Request timeout in seconds (default: 30)
TIMEOUT_SECONDS=30
