[run]
source = src/flight_delay_explorer
omit =
    tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */virtualenvs/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError
    if TYPE_CHECKING:

fail_under = 90
show_missing = true
skip_covered = false

[html]
directory = htmlcov
